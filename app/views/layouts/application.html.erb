        <!--Favicon-->
        <!-- <link rel="shortcut icon" href="/assets/img/K.jpg" type="image/x-icon"> -->
        <link rel="shortcut icon" href="/assets/img/profile-picture-1.jpg" type="image/x-icon">
  </head>

  <body id="page-top" class="pace-done">

    <!-- Navigation start -->
      <%= render "layouts/navigation_top" %>
    <!-- Navigation end -->
    
    <!-- <p class="notice">< %= notice % ></p> -->
    <!-- <p class="alert">< %= alert % ></p> -->

        <!-- LOGIN MESSAGE -->
    <% flash.each do |name, msg| %>
        <div class="alert-contain navbar-message">
          <div class="alert-new alert-<%= name %> alert-dismissible show"  role="alert">
              <%= msg %>
              <button type="button" class="close" data-dismiss="alert" aria-label="Close"  id="x">
                <span aria-hidden="true">&times;</span>
              </button>
          </div>
        </div>
    <% end %>

    <%= yield %>

    <!-- Footer start -->
      <%= render "layouts/footer" %>
    <!-- Footer end -->

    <!-- Scroll to top -->
    <a href="#page-top" class="page-scroll scroll-to-top bg-base-color"><i class="fa fa-angle-up"></i></a>
    <!-- JS du template -->
        
        
